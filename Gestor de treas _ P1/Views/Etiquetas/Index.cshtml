@model IEnumerable<Gestor_de_treas___P1.Models.Etiqueta>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Color)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Color)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EtiquetaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EtiquetaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EtiquetaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
